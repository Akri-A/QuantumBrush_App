{
    "name": "Damping",
    "id": "damping2", 
    "author": "Joao",
    "version": "1.0.0",
    "description": "Pixels are encoded as qubits with amplitude damping; click and drag to select the region.\nRadius: brush size.\nNumber of Drops: number of color segments.\nStrength: effect intensity.\nInvert Luminosity: pushes segments toward white.",
    "dependencies": {
        "numpy": ">=2.1.0",
        "colorsys": "",
        "qiskit": ""
    },
    "user_input": {
        "Number of Drops": {
            "type": "int",
            "min": 1,
            "max": 15,
            "default": 5 
        },
        "Radius": {
            "type": "int",
            "min": 0,
            "max": 100,
            "default": 20 
        },
        "Strength": {
            "type": "float",
            "min": 0.0,
            "max": 1.0,
            "default": 1.0 
        },
        "Invert Luminosity": {
            "type": "bool",
            "default": false 
        }
    },
    "stroke_input": {
        "image_rgba": "array",
        "path": "array",
        "clicks": "array"
    },
    "flags": {
        "smooth_path": false
    }
}