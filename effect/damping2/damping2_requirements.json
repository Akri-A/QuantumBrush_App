{
    "name": "QDamp",
    "id": "damping2", 
    "author": "Joao",
    "version": "1.0.0",
    "description": "This brush applies sort of an amplitude damping to each segment. Once can create multiple segments by clicking and draging multiple times (no more than 20 times).\nRadius: brush size.\nStrength: effect intensity.\nInvert Luminosity: pushes segments toward white.",
    "dependencies": {
        "numpy": ">=2.1.0",
        "colorsys": "",
        "qiskit": ""
    },
    "user_input": {
        "Radius": {
            "type": "int",
            "min": 0,
            "max": 100,
            "default": 20 
        },
        "Strength": {
            "type": "float",
            "min": 0.0,
            "max": 1.0,
            "default": 1.0 
        },
        "Invert Luminosity": {
            "type": "bool",
            "default": false 
        }
    },
    "stroke_input": {
        "image_rgba": "array",
        "path": "array",
        "clicks": "array"
    },
    "flags": {
        "smooth_path": false
    }
}