{
    "name": "Damping",
    "id": "damping", 
    "author": "Joao",
    "version": "1.0.0",
    "description": "Each groups of pixel get encoded as a qubit undergoing an amplitude damping circuit.",
    "dependencies": {
        "numpy": ">=2.1.0",
        "colorsys": "",
        "qiskit": ""
    },
    "user_input": {
        "Number of Drops": {
            "type": "int",
            "min": 0,
            "max": 10,
            "default": 5 
        },
        "Radius": {
            "type": "int",
            "min": 0,
            "max": 100,
            "default": 20 
        },
        "Strength": {
            "type": "float",
            "min": 0,
            "max": 1,
            "default": 1 
        },
        "Invert Luminosity": {
            "type": "bool",
            "default": false 
        }
    },
    "stroke_input": {
        "image_rgba": "array",
        "path": "array",
        "clicks": "array"
    },
    "flags": {
        "smooth_path": true
    }
}