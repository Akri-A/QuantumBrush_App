{
    "name": "Steerable",
    "id": "steerable",
    "author": "Luminists Team",
    "version": "1.0.0",
    "description": "Draw two to three regions: source, target, and optional paste region. The quantum circuit will steer the source region to the target region, and modifies the pixels of the paste region or source region if paste region is not specified.",
    "dependencies": {
        "numpy": ">=2.1.0",
        "qiskit": "",
        "jax": "",
        "pennylane": ""
    },
    "user_input": {
        "Controls": {
            "type": "int",
            "min": 2,
            "max": 4,
            "default": 2
        },
        "timesteps": {
            "type": "int",
            "min": 10,
            "max": 100,
            "default": 40
        },
        "t": {
            "type": "float",
            "min": 0.0,
            "max": 1.5,
            "default": 0.8
        },
        "Souce=Paste": {
            "type": "bool",
            "default": true
        },
        "show source & target": {
            "type": "bool",
            "default": false
        },
        "show color": {
            "type": "color",
            "default": "#FF0000"
        }
    },
    "stroke_input": {
        "image_rgba": "array",
        "clicks": "array",
        "path": "array"
    },
    "flags": {}
}