{
    "name": "Super cool effect",
    "id": "id_without_spaces", 
    "author": "creator",
    "version": "1.0.0",
    "description": "Small description of the effect.",
    "dependencies": {
        "numpy": ">=2.1.0"
    },
    "user_input": {
        "Radius": {
            "type": "int",
            "min": 0,
            "max": 100,
            "default": 20 
        },
        "Strenght": {
            "type": "float",
            "min": 0,
            "max": 1,
            "default": 1
        },
        "Color": {
            "type": "color",
            "default": "#FF0000"
        }
    },
    "stroke_input": {
        "image_rgba": "array",
        "path": "array",
        "clicks": "array"
    },
    "flags": {
        "smooth_path": true
    }
}